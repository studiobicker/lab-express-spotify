<h1>Tracks</h1>

<main class="site-main">
  <div class="wrap">

    <table class="tracks">

      {{#each allTracks}}
      <tr>
        <td>
          {{this.name}}
        </td>
        <td>
          {{#if this.preview_url}}
          <audio controls src={{this.preview_url}}>
            Your browser does not support the
            <code>audio</code> element.
          </audio>
          {{/if}}
        </td>
      </tr>
      {{/each}}
    </table>
  </div>
</main>